{
  "$schema": "http://json-schema.org/schema#",
  "$id": "http://sudomoin-pavel-project-mesto.tk/schemas/card.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 30
    },
    "link": {
      "type": "string",
      "format": "url"
    }
  },
  "required": ["name", "link"],
  "additionalProperties": false
}